    "window.zoomLevel": 3,
    "workbench.colorTheme": "Atom One Dark",
    "workbench.editor.tabSizing": "fit",
    "workbench.statusBar.visible": true,
    "workbench.layoutControl.enabled": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#363d45",
        "statusBar.foreground": "#fffefeea",
        "statusBar.border": "#0b0b0b00",
        "statusBarItem.hoverBackground": "#87b6aa7a",
        "statusBarItem.remoteBackground": "#8ac362",
        "statusBarItem.remoteForeground": "#000000",
        "editor.lineHighlightBackground": "#62616d4a",
