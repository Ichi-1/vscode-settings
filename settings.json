{
    /* Python Type Hinting and Linting */
    "python.languageServer": "Pylance",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "autoDocstring.docstringFormat": "sphinx-notypes",
    "python.autoComplete.extraPaths": [],
    "python.analysis.extraPaths": ["./sources"],
    /* WSL settings */ 
    "remote.WSL1.connectionMethod": "wslExeProxy",
    "remote.downloadExtensionsLocally": true,
    "http.proxyAuthorization": null,
    "http.proxyStrictSSL": true,
    /*Explorer */
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    /* Terminal */ 
    "terminal.integrated.rightClickBehavior": "paste",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "MesloLGS NF Regular",
    "terminal.integrated.defaultProfile.linux": "zsh",
    /* Workbenc & Editor customization */
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 3,
    "workbench.colorTheme": "Atom One Dark",
    "workbench.editor.tabSizing": "shrink",
    "workbench.statusBar.visible": true,
    "workbench.layoutControl.enabled": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#2c3441",
        "statusBarItem.remoteBackground": "#8eb473",
        "statusBarItem.remoteForeground": "#000000",
        "statusBar.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#62616d4a",
        "editor.background": "#252a34",
        "sideBar.background": "#1e2229",
        "list.hoverBackground": "#62616d4a",
        "editorCursor.foreground": "#fff",
        "editorLineNumber.activeForeground": "#efe637",
        "editorLineNumber.foreground": "#ffffff7f"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.lineNumbers": "relative",
    "editor.wordWrap": "on",
    "editor.renderLineHighlight": "line",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line-thin",
    "editor.detectIndentation": false,
    "editor.snippetSuggestions": "bottom",
    "editor.guides.bracketPairsHorizontal": false,
    "editor.minimap.enabled": false,
    "editor.fontLigatures": true,
    "editor.fontWeight": "450",
    "editor.fontFamily": "Fira Code",
    "editor.codeLensFontSize": 11,
